{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["prettier", "google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "quotes": ["error", "single"],
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "indent": "off",
    "comma-dangle": "off",
    "max-len": "off"
  }
}
